{
  "name": "corvo-astral",
  "version": "1.0.0",
  "description": "A Discord Bot that serves as an Wakfu helper",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "import:equipment": "npm run build && node dist/utils/importEquipment.js",
    "identify:actions": "npm run build && node dist/utils/identifyActions.js",
    "build": "babel src -d dist",
    "lint": "eslint .",
    "test": "jest tests/**.test.js"
