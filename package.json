{
  "name": "corvo-astral",
  "version": "2.0.0",
  "description": "A Discord Bot that serves as an Wakfu helper",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "babel src -d dist",
    "import": "npm run build && node dist/importers",
    "lint": "eslint .",
    "test": "jest tests/ --runInBand",
    "clean": "rm package-lock.json && rm -rf node_modules"
  },
  "keywords": [
    "wakfu",
    "discord",
    "nodejs"
  ],
  "author": "Mark Kop",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "axios": "^0.21.1",
    "codecov": "^3.7.2",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "mongoose": "^5.10.5",
    "node-cron": "^2.0.3",
    "puppeteer": "^5.5.0",
    "puppeteer-extra": "^3.1.15",
    "puppeteer-extra-plugin-stealth": "^2.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "^26.0.13",
    "babel-jest": "^26.3.0",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsdoc": "^9.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node"
  }
}
